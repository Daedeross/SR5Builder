<Window x:Class="SR5Builder.Dialogs.OkCancelDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:views="clr-namespace:SR5Builder.Views"
        xmlns:helpers="clr-namespace:Helpers" SizeToContent="WidthAndHeight"
        ResizeMode="NoResize" WindowStyle="ToolWindow"
        helpers:DialogCloser.DialogResult="{Binding Result}">
    <Grid Margin="5">
        <views:OKCancelDialogView DataContext="{Binding}" />
    </Grid>
</Window>
